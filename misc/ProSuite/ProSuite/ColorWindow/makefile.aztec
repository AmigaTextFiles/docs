COLOR=	color.o colordat.o hsl.o main.o

.c.o:
	cc $(CFLAGS) +C +D +Iram:prosuite.pre +l $* -o $*.o

.asm.o:
	as $(AFLAGS) $* -o $*.o

color:	$(COLOR)
	ln -w -o color $(COLOR) -lm32 -lc32

include:
	cc +Hprosuite.pre dh0:amiga/include/prosuite/prosuite.h
	copy prosuite.pre ram:prosuite.pre

all:
	make include
	delete #?.o
	make

o:	$(COLOR)

	
