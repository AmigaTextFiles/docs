FILEIO=	alerts.o chipdata.o fileio.o filename.o filesupp.o globfio.o main.o \
	opendata.o pointers.o reqsupp.o strings.o volname.o

.c.o:
	cc $(CFLAGS) +C +D +Iram:prosuite.pre +l $* -o $*.o

.asm.o:
	as $(AFLAGS) $* -o $*.o

fileio:	$(FILEIO)
	ln -w -o fileio +cdb $(FILEIO) -lm32 -lc32

include:
	cc +Hprosuite.pre :amiga/include/prosuite/prosuite.h
	copy prosuite.pre ram:prosuite.pre

xinclude:
	copy prosuite.pre ram:prosuite.pre

all:
	make include
	delete #?.o
	make

o:	$(FILEIO)


