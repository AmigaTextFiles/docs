{
	static	W3D_Triangle   tri;
	static	W3D_Color		color;
	int		i,j;
	float	swidth,sheight;
	float	x0,y0;

	float	fixsize;

	if	(picture==NULL)	return;

	if	(picture->width==640 && picture->height==480)
	{
		DisplayFSPicture640x480(engine,picture,opacity);
		return;
	}

	swidth=engine->width;
	sheight=engine->height;

	x0=(engine->width/2)-(picture->width/2);
	y0=(engine->height/2)-(picture->height/2);

	opacity=1.0-opacity;

	// calculate edge fix parameter
	if	(opacity>=0.99)
	{
		W3D_SetState(engine->context,W3D_BLENDING,		    W3D_DISABLE);
		fixsize=0.0;
	}
	else
	{
		fixsize=0.0;
		W3D_SetState(engine->context,W3D_BLENDING,		    W3D_ENABLE);
	}


	color.r=1.0;
	color.g=1.0;
	color.b=1.0;
	color.a=opacity;

	W3D_SetBlendMode(engine->context,W3D_SRC_ALPHA,W3D_ONE_MINUS_SRC_ALPHA);

	W3D_SetState(engine->context,W3D_ZBUFFER,			W3D_FALSE);
	W3D_SetState(engine->context,W3D_ZBUFFERUPDATE,		W3D_FALSE);
	W3D_SetCurrentColor(engine->context,&color);

    W3D_SetState(engine->context,W3D_TEXMAPPING,W3D_ENABLE);
	W3D_SetState(engine->context,W3D_GOURAUD,W3D_DISABLE);

	W3D_SetState(engine->context,W3D_DITHERING,W3D_ENABLE);

	if	(W3D_LockHardware(engine->context)!=W3D_SUCCESS)	return;

	// draw rectangles
	for	(j=0;j<picture->numy;j++)
	{
		for	(i=0;i<picture->numx;i++)
		{
			float	x1,y1,x2,y2,u1,v1,u2,v2;

			x1=x0+i*picture->tsize;		   y1=y0+j*picture->tsize;
			x2=x0+(i+1)*picture->tsize-fixsize;    y2=y0+(j+1)*picture->tsize-fixsize;
			u1=0.0;						v1=0.0;
			u2=picture->tsize-1.0;		v2=picture->tsize-1.0;

			if	(IsOnScreen(x1,y1,x2,y2,swidth,sheight))
			{
				if	(x1<0)
				{
					float	t;

					t=CalculateFraction(x1,0,x2);
					u1=u1+t*(u2-u1);
					x1=0;
				}
				// clip to right border (only U is clipped, X is clamped)
				if	(x2>(swidth-1))
				{
					float	t;

					t=CalculateFraction(x2,swidth-1,x1);
					u2=u2+t*(u1-u2);
					x2=swidth-1;
				}


				if	(y1<0)		// it's the same as if (x2<0)
				{
					float	t;

					t=CalculateFraction(y1,0,y2);
					v1=v1+t*(v2-v1);
					y1=0;
				}
				// clip to bottom border (only V is clipped, Y is clamped)
				if	(y2>(sheight-1))
				{
					float	t;

					t=CalculateFraction(y2,sheight-1,y1);
					v2=v2+t*(v1-v2);
					y2=sheight-1;
				}

				tri.v1.x=x1;	  tri.v1.y=y1;			tri.v1.z=0.1;	tri.v1.w=1.0;
				tri.v2.x=x2;	  tri.v2.y=y1;			tri.v2.z=0.1;	tri.v2.w=1.0;
				tri.v3.x=x2;	  tri.v3.y=y2;		tri.v3.z=0.1;	tri.v3.w=1.0;

				tri.v1.u=u1;	   tri.v1.v=v1;
				tri.v2.u=u2;     tri.v2.v=v1;
				tri.v3.u=u2;     tri.v3.v=v2;

				tri.tex=picture->textures[j*picture->numx+i]->w3d_texture;

				W3D_DrawTriangle(engine->context,&tri);

				tri.v2.x=x1;     tri.v2.y=y2;	 tri.v2.z=0.1;	 tri.v2.w=1.0;

				tri.v2.u=u1;	 tri.v2.v=v2;

				W3D_DrawTriangle(engine->context,&tri);
			}
		}
	}

	W3D_SetState(engine->context,W3D_DITHERING,W3D_DISABLE);

	W3D_SetState(engine->context,W3D_ZBUFFER,			W3D_ENABLE);
	W3D_SetState(engine->context,W3D_ZBUFFERUPDATE,		W3D_ENABLE);
	W3D_SetState(engine->context,W3D_BLENDING,			W3D_DISABLE);

	W3D_SetBlendMode(engine->context,W3D_SRC_ALPHA,W3D_ONE_MINUS_SRC_ALPHA);
	W3D_SetBlendMode(engine->context,W3D_ONE,W3D_ONE);

	W3D_UnLockHardware(engine->context);

