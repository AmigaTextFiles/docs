    xdef    _set_dtr_rts
    section text,code

_set_dtr_rts:

dtr:
    tst.l       d0
    beq.s       unset_dtr
set_dtr
    bset.b  #7,$bfd000

rts:
    tst.l       d1
    beq.s       unset_rts

set_rts
    bset.b  #6,$bfd000
    rts

unset_dtr:
    bclr.b  #7,$bfd000
    bra.s       rts

unset_rts:
    bclr.b  #6,$bfd000
    rts

    END

