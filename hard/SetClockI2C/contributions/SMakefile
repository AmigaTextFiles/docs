#
##
#

TARGET = SetClock

RM	= delete force quiet
CC	= sc
CFLAGS	= NOVER NOSTKCHK PARMS=r STRMER OPT OPTINLOCAL
LDFLAGS	= NOVER LINK SMALLCODE SMALLDATA

OBJS	= SetClockI2C.o rtcI2C.o

$(TARGET): $(OBJS)
	$(CC) $(LDFLAGS) TO $(TARGET) $(OBJS)

clean:
	-$(RM) $(TARGET) $(OBJS) #?.lnk

# dependencies

SetClockI2C.o: SetClockI2C.c rtcI2C.h clockpref.h
rtcI2C.o: rtcI2C.c rtcI2C.h clockpref.h
