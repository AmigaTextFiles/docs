ask "Vill du installera The World på din hårddisk (HD1:)?"
if warn
   makedir hd1:The_World
   copy icons/icon0.info to hd1:The_World.info quiet
   copy #? to hd1:The_World/ quiet
   delete hd1:The_World/install#? quiet
   delete hd1:The_World/icons/#? quiet
   delete hd1:The_World/icons quiet
else
   echo "OK!"
endif
echo "Tryck på stängningsknappen för att bli av med fönstret."
