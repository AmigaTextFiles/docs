; $VER: AmigaOSGuide-Version V0.15 (28.01.96)

C:VERSION AmigaOSGuide >ENV:Version
C:VERSION AmigaOSGuide_Index >ENV:Version1
C:VERSION DOS >ENV:Version2
C:VERSION Lexikon >ENV:Version3
C:VERSION Schnittstellen >ENV:Version4
C:VERSION DOS-Beispiele >ENV:Version5

C:REQUESTCHOICE >NIL: "AmigaOSGuide" "$Version*N$Version1*N$Version2*N$Version3*N$Version4*N$Version5*N*N© by Thorsten Willert" "OK"

UNSETENV Version
UNSETENV Version1
UNSETENV Version2
UNSETENV Version3
UNSETENV Version4
UNSETENV Version5

