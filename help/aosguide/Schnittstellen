@node MonitorSch "AmigaOSGuide Schnittstellen:  Monitor-Schnittstelle"
@prev MausSch
@{fg shine}
     ____________________________________________________________________
@{fg text}
     Signale und Pin Belegungen der Monitor Schnittstelle eines A2000.@{fg shine}
     ____________________________________________________________________
@{fg text}
@{b}
        Pinnummer     @{" Signal " link MonPegel}               Funktion
       ________________________________________________________________

        23pol.
       ________________________________________________________________@{ub}

         1            @{" XCLK   " link MXCLK}                External Clock
         2            @{" XCLKEN " link MXCLKEN}                External Clock Enable
         3            @{" RED    " link MRED}                Analog Red
         4            @{" GREEN  " link MGREEN}                Analog Green 
         5            @{" BLUE   " link MBLUE}                Analog Blue
         6            @{" DI     " link MDI}                Digital Intensity
         7            @{" DB     " link MDB}                Digital Blue
         8            @{" DG     " link MDG}                Digital Green
         9            @{" DR     " link MDR}                Digital Red
        10            @{" CSYNC  " link MCSYNC}                Composite Sync
        11            @{" HSYNC  " link MHSYNC}                Horizontal Sync
        12            @{" VSYNC  " link MVSYNC}                Vertical Sync
        13            @{" GNDRTN " link MGNDRTN}                Return for  XCLKEN
        14            @{" ZD     " link MZD}                Zero Detect
        15            @{" C1     " link MC1}                Clock Out
        16            @{" GND    " link GND}                Signalmasse
        17            @{" GND    " link GND}                Signalmasse
        18            @{" GND    " link GND}                Signalmasse
        19            @{" GND    " link GND}                Signalmasse
        20            @{" GND    " link GND}                Signalmasse
        21            @{" -12V   " link M-12V}                -12V DC
        22            @{" +12V   " link M+12V}                +12V DC
        23            @{" +5V    " link M+5V}                +5V DC
       ________________________________________________________________


@next Parallele
@endnode
