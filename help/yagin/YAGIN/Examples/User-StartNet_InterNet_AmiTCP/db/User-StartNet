.key MODE
.bra [
.ket ]
.def MODE online

; $VER: User-StartNet 1.0 (5.7.96) Arndt van der Molen


;BEGIN INET
execute InterNet:INet/SetUp_INet

IF [MODE] EQ "online"
  ; Advise SMTPPost to NOT queue outgoing _mail_ because we are in online mode.
  setenv SMTPAlwaysQueue	no
ENDIF
;END INET


;BEGIN UUCP
execute InterNet:UUCP/SetUp_UUCP

IF [MODE] EQ "online"
  ; Advise (New)PostNews to NOT queue outgoing _news_ because we are in online mode.
  unsetenv PNS_OFFLINE
ENDIF
;END UUCP


;BEGIN GRN
execute InterNet:GRN/SetUp_GRN
;END   GRN
