
; $VER: Get_NewsList 1.0 (5.7.95) Arndt van der Molen

echo "*NGetting the NEWSGROUP list from your server '$nntpserver' ...*N"

changetaskpri -2

INet:c/getactive $nntpserver UULIB:News/Active_Server

changetaskpri 0

echo "*NNEWSGROUP list ist stored in 'UULIB:News/Active_Server'!"
echo "*NReady!*N"
