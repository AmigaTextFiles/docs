/* $VER: MO4.1 40.1 (18.6.99)
 *
 * 4.1GB 5¼" MO file system entry
 *
 * This file enables FastFileSystem on a 4.1GB 5¼" MO disk cartridge.
 */

/* 4.1GB 5¼" MO disks have 3973952 512-byte sectors on each side. 64×62093 = 3973952 */

FileSystem	= L:FastFileSystem_43.20	/* Any FFS version should work */
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 64
SectorsPerBlock	= 1
SectorSize	= 512
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 62092
Buffers		= 128		/* Number of 512-byte filesystem buffers */
BufMemType	= 1
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the MO4.1 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
