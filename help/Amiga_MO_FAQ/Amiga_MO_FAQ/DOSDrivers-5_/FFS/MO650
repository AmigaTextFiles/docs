/* $VER: MO650 40.1 (21.6.99)
 *
 * 650MB 5¼" MO file system entry
 *
 * This file enables FastFileSystem on a 650MB 5¼" MO disk cartridge.
 */

/* 650MB 5¼" MO disks have 314569 1024-byte sectors on each side. 314569 is a
   prime number, so if all sectors are to be used, must have BlocksPerTrack = 1 */

FileSystem	= L:FastFileSystem_43.20	/* At least FFS 40.1 required for 1K sector support */
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 1
SectorsPerBlock	= 1
SectorSize	= 1024
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 314568
Buffers		= 128		/* Number of 1024-byte filesystem buffers */
BufMemType	= 1
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the MO650 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
