/* $VER: SF2.6 40.1 (21.6.99)
 *
 * 2.6GB 5¼" MO file system entry, for superfloppy type MS-DOS disks.
 *
 * This file enables CrossDOSFileSystem on a 2.6GB 5¼" MO disk cartridge.
 */

/* 2.6GB 5¼" MO disks have 1273011 1024-byte sectors on each side. */

FileSystem	= L:CrossDOSFileSystem
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 1
SectorsPerBlock	= 1
SectorSize	= 1024
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 1
Interleave	= 0
LowCyl		= 0
HighCyl		= 1273010
Buffers		= 128		/* Number of 1024-byte filesystem buffers */
BufMemType	= 1
DosType		= 0x4D534400
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the SF2.6 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
