/* $VER: SFS1.2 40.1 (21.6.99)
 *
 * 1.2GB 5¼" MO file system entry
 *
 * This file enables SFS on a 1.2GB 5¼" MO disk cartridge.
 */

/* 1.2GB 5¼" MO disks have 1163337 512-byte sectors on each side. 93×12509 = 1163337 */

FileSystem	= L:SmartFileSystem
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 93
SectorsPerBlock	= 1
SectorSize	= 512
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 12508
Buffers		= 128		/* Number of 512-byte filesystem buffers */
BufMemType	= 1
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the SFS1.2 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
