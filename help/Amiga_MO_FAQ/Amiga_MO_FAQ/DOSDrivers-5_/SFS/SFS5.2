/* $VER: SFS5.2 40.1 (21.6.99)
 *
 * 5.2GB 5¼" MO file system entry
 *
 * This file enables SFS on a 5.2GB 5¼" MO disk cartridge.
 */

/* 5.2GB 5¼" MO disks have 1263472 2048-byte sectors on each side. 56×22562 = 1263472 */

FileSystem	= L:SmartFileSystem
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 56
SectorsPerBlock	= 1
SectorSize	= 2048
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 22561
Buffers		= 128		/* Number of 2048-byte filesystem buffers */
BufMemType	= 1
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the SFS5.2 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
