/* $VER: SFS600 40.1 (21.3.99)
 *
 * 600MB 5¼" MO file system entry
 *
 * This file enables SFS on a 600MB 5¼" MO disk cartridge.
 */

/* 600MB 5¼" MO disks have 276999 512-byte sectors on each side. 61*9459 = 576999 */

FileSystem	= L:SmartFileSystem
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 61
SectorsPerBlock	= 1
SectorSize	= 512
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 9458
Buffers		= 128		/* Number of 512-byte filesystem buffers */
BufMemType	= 1
StackSize	= 4096
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the SFS600 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
