/* $VER: SF640 40.3 (4.10.99)
 *
 * 640MB MO file system entry, for superfloppy type MS-DOS disks.
 *
 * This file enables CrossDOSFileSystem on a 640MB MO disk cartridge.
 */

/* 640MB MO disks have 310352 2048-byte sectors. 18256*17 = 310352 */

FileSystem	= L:CrossDOSFileSystem
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 17
SectorsPerBlock	= 1
SectorSize	= 2048
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 1
Interleave	= 0
LowCyl		= 0
HighCyl		= 18255
Buffers		= 128		/* Number of 2048-byte filesystem buffers */
BufMemType	= 1
DosType		= 0x4D534400
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the SF640 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
