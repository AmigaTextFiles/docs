/* $VER: MO128 40.3 (21.3.99)
 *
 * 128MB MO file system entry
 *
 * This file enables FastFileSystem on a 128MB MO disk cartridge.
 */

/* 128MB MO disks have 248826 512-byte sectors. 2202*113 = 248826 */

FileSystem	= L:FastFileSystem_43.20	/* Any FFS version should work */
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 113
SectorsPerBlock	= 1
SectorSize	= 512
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 2201
Buffers		= 128		/* Number of 512-byte filesystem buffers */
BufMemType	= 1
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the MO128 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
