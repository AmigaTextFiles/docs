/* $VER: MO230 40.4 (21.3.99)
 *
 * 230MB MO file system entry
 *
 * This file enables FastFileSystem on a 230MB MO disk cartridge.
 */

/* 230MB MO disks have 446325 512-byte sectors. 17853*25 = 446325 */

FileSystem	= L:FastFileSystem_43.20	/* Any FFS version should work */
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 25
SectorsPerBlock	= 1
SectorSize	= 512
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 17852
Buffers		= 128		/* Number of 512-byte filesystem buffers */
BufMemType	= 1
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the MO230 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
