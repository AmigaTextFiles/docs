/* $VER: MO540 40.4 (4.10.99)
 *
 * 540MB MO file system entry
 *
 * This file enables FastFileSystem on a 540MB MO disk cartridge.
 */

/* 540MB MO disks have 1041500 512-byte sectors. 20830*50 = 1041500 */

FileSystem	= L:FastFileSystem_43.20	/* Any FFS version should work */
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 50
SectorsPerBlock	= 1
SectorSize	= 512
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 20829
Buffers		= 128		/* Number of 512-byte filesystem buffers */
BufMemType	= 1
StackSize	= 600
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the MO540 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
