/* $VER: BSD540 40.2 (21.3.99)
 *
 * 540MB MO file system entry
 *
 * This file enables the Berkeley Fast File System (BFFS) as used by NetBSD on
 * a 540MB MO disk cartridge.
 */

/* 540MB MO disks have 1041500 512-byte sectors. 3829*272 = 1041760. Is this
 * correct?
 */

FileSystem	= L:BFFSFileSystem
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 272
SectorsPerBlock	= 1
SectorSize	= 512
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= -1		/* UNIX partition (-1 = no disk label) */
PreAlloc	= 0		/* BFFS startup flags */
Interleave	= 0
LowCyl		= 2
HighCyl		= 3829
Buffers		= 128		/* Number of 512-byte filesystem buffers */
BufMemType	= 1
DosType		= 0x4E425507	/* 'BFFS' - use another type if you want */
StackSize	= 512
Priority	= 5
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the BSD540 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
