/* $VER: MAC640 40.3 (21.3.99)
 *
 * 640MB MO file system entry
 *
 * This file enables CrossMacFileSystem on a 640MB MO disk cartridge.
 */

/* 640MB MO disks have 310352 2048-byte sectors. 4564*68 = 310352 */

FileSystem	= L:CrossMacFileSystem
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 68
SectorsPerBlock	= 1
SectorSize	= 2048
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 0
Interleave	= 0
LowCyl		= 0
HighCyl		= 4563
Buffers		= 96		/* Number of 2048-byte filesystem buffers */
BufMemType	= 1
DosType		= 0x4D414300
StackSize	= 2000
Priority	= 5
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the MAC640 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
