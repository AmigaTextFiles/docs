/* $VER: SFS640 40.2 (21.3.99)
 *
 * 640MB MO file system entry
 *
 * This file enables SFS on a 640MB MO disk cartridge.
 */

/* 640MB MO disks have 310352 2048-byte sectors. 18256*17 = 310352 */

FileSystem	= L:SmartFileSystem
Flags		= 0
Surfaces	= 1
BlocksPerTrack	= 17
SectorsPerBlock	= 1
SectorSize	= 2048
Mask		= 0x7FFFFFFE	/* Change to suit your system and SCSI controller */
MaxTransfer	= 0xFFFFFFFE	/* Change to suit your system and SCSI controller */
Reserved	= 2
Interleave	= 0
LowCyl		= 0
HighCyl		= 18255
Buffers		= 96		/* Number of 2048-byte filesystem buffers */
BufMemType	= 1
StackSize	= 4096
Priority	= 10
GlobVec		= -1

/* The Device and Unit fields are controlled by tooltypes in the SFS640 icon.
 *
 * Device	= scsi.device
 * Unit		= 4
 */
