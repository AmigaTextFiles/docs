; Example-29.1

FAILAT 21
LAB start-here
  COPY >nil: df0:test-file Ram:
  IF FAIL
    SKIP fail-message
  ENDIF
  ECHO "*"Test-file*" copied"
  ASK "Copy to another disk? (Y/N)"
  IF NOT WARN
    SKIP end-file
  ELSE
    SKIP BACK start-here
  ENDIF

LAB fail-message
  ECHO "*"test-file*" not on df0:"

LAB end-file
  ECHO "script has finished"
