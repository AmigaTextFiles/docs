; Example-66.4

LAB Start

ECHO "Select a file name from the requester for typing"

REQUESTFILE >Env:Selection

IF WARN
  ECHO "CANCEL Selected"
  SKIP Another
ENDIF

IF $Selection EQ ""
  ECHO "You clicked OK without a file name selection!"
  SKIP Another
ELSE 
  TYPE $Selection
ENDIF

UNSETENV Selection

LAB Another

ASK "Type another file? (Y/N)"

IF WARN
  SKIP BACK Start
ENDIF
