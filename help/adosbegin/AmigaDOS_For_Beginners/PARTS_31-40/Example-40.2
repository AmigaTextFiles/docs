; Example-40.2
;
LAB Start
ECHO "*nEnter a number - try one below 99 then one over 99"
SETENV >nil: number ?
ECHO "*nYou entered " NOLINE
GETENV number
IF VAL $number GT 99
  ECHO "which IS greater than 99"
ELSE
  ECHO "which is NOT greater than 99"
ENDIF
ASK "*nAnother ? (Y/N)"
IF WARN
  SKIP BACK Start
ENDIF

UNSETENV number