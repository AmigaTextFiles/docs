@{" Error  1 Program Not Found                  " LINK "1"}
@{" Error  2 Execution Halted                   " LINK "2"}
@{" Error  3 Insufficient Memory                " LINK "3"}
@{" Error  4                                    " LINK "4"}
@{" Error  5 Unmatched Quote                    " LINK "5"}
@{" Error  6 Unterminated Comment               " LINK "6"}
